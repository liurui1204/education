<?xml version="1.0" encoding="UTF-8"?>
<application>
    <module name="h4aIdentity">
        <!-- 海关H4A接口服务地址示例，以下为示例地址。请按照实际环境进行配置,请与H4A人员联系 -->
        <group name="wsAddress">
            <!-- 身份服务 -->
            <configValue key="OguReaderService">
                http://192.168.31.156:8085/CupaaDistributedService/OguReaderService.svc
            </configValue>
            <!-- 授权服务 -->
            <configValue key="AccreditReaderService">
                http://192.168.31.156:8085/CupaaDistributedService/AccreditReaderService.svc
            </configValue>
            <!-- 应用注册服务 -->
            <configValue key="RegisterAppService">
                http://192.168.31.156/AppRegisterService/RegisterAppReaderService.svc
            </configValue>
        </group>
    </module>
    <module name="passportSSO">
        <!-- 海关H4A单点登录地址示例，以下为示例地址。请按照实际环境进行配置 -->
        <group name="url">
            <configValue key="authenticateUrl">http://192.168.31.156/Passport/SsoLogin.aspx</configValue>
            <configValue key="logOffUrl">http://192.168.31.156/Passport/LogOff.aspx</configValue>
        </group>
    </module>
</application>

